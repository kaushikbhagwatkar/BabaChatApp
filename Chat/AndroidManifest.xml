<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.baba.chat"
android:versionCode="1"
android:versionName="1.0" >
 
<uses-sdk
android:minSdkVersion="11"
android:targetSdkVersion="17" />
 
<uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
 <uses-permission android:name="android.permission.RECORD_AUDIO"/>
 <uses-permission  android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
     <uses-permission android:name="android.permission.CAMERA" />
 <uses-feature android:name="android.hardware.camera" />
 <uses-feature android:name="android.hardware.camera.autofocus" />
 <uses-feature android:name="android.hardware.camera.front"/>
 
<application
android:allowBackup="true"
android:icon="@drawable/ic_launcher"
android:label="@string/app_name"
android:theme="@style/AppTheme" >

    // 
    
<activity
android:name=".MainActivity"
android:label="@string/app_name" >
<intent-filter>
<action android:name="com.baba.chat.MAINACTIVITY" />
 
<category android:name="android.intent.category.DEFAULT" />
</intent-filter>
</activity>

<activity
android:name=".AudioMainActivity"
android:label="@string/app_name" >
<intent-filter>
<action android:name="com.baba.chat.AUDIOMAINACTIVITY" />
 
<category android:name="android.intent.category.DEFAULT" />
</intent-filter>
</activity>

<activity
android:name=".AudioDoubt"
android:label="@string/app_name"
android:theme="@android:style/Theme.DeviceDefault.Dialog.NoActionBar"

 >

<intent-filter>
<action android:name="com.baba.chat.AUDIODOUBT" />

     
     <category android:name="android.intent.category.DEFAULT" />
</intent-filter>
</activity>


   
<activity
android:name=".FirstMainActivity"
android:label="@string/app_name" >
<intent-filter>
<action android:name="com.baba.chat.FIRSTMAINACTIVITY" />
 
<category android:name="android.intent.category.DEFAULT" />
</intent-filter>
</activity>

  
<activity
android:name=".Doubt"
android:label="@string/app_name" >
<intent-filter>
<action android:name="com.baba.chat.DOUBT" />
 
<category android:name="android.intent.category.DEFAULT" />
</intent-filter>
</activity>


   
<activity
android:name=".Help"
android:label="@string/app_name" >
<intent-filter>
<action android:name="com.baba.chat.HELP" />
 
<category android:name="android.intent.category.DEFAULT" />
</intent-filter>
</activity>

   
<activity
android:name=".Settings"
android:label="@string/app_name" >
<intent-filter>
<action android:name="com.baba.chat.SETTINGS" />
 
<category android:name="android.intent.category.DEFAULT" />
</intent-filter>
</activity>





<activity
android:name=".Splash"
android:label="@string/app_name" 
android:screenOrientation="landscape"

>
<intent-filter>
<action android:name="android.intent.action.MAIN" />
 
<category android:name="android.intent.category.LAUNCHER" />
</intent-filter>
</activity>


<activity
android:name=".Login"
android:label="@string/app_name"
android:screenOrientation="landscape"
 >
<intent-filter>
<action android:name="com.baba.chat.LOGIN" />
 
<category android:name="android.intent.category.DEFAULT" />
</intent-filter>
</activity>


<activity
android:name=".Mid"
android:label="@string/app_name" >
<intent-filter>
<action android:name="com.baba.chat.MID" />
 
<category android:name="android.intent.category.DEFAULT" />
</intent-filter>
</activity>



<activity
android:name=".Signin"
android:label="@string/app_name" >
<intent-filter>
<action android:name="com.baba.chat.SIGNIN" />
 
<category android:name="android.intent.category.DEFAULT" />
</intent-filter>
</activity>


<activity
android:name=".TestConnection"
android:label="@string/app_name" >
<intent-filter>
<action android:name="com.baba.chat.TESTCONNECTION" />
 
<category android:name="android.intent.category.DEFAULT" />
</intent-filter>
</activity>





</application>
 
</manifest>